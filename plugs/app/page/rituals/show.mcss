DarkCrystal -show {
  position: relative

  div.Back {
    position: absolute
    top: 2.4rem
    left: 1rem

    i.fa {
      cursor: pointer

      :hover {
        transition: color 0.2s ease-in
        color: rebeccapurple
      }
    }
  }

  h2 {
    margin: 0
  }


  div.DarkCrystalShow {
    margin-top: 1rem
  }
}
